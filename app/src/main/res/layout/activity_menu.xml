<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.dani.etakemongo.DevelopFrontends.Menu"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp"
    android:background="@drawable/fondo">

    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="6dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="120dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="302dp"
                android:layout_height="match_parent"
                android:orientation="vertical"></LinearLayout>

            <LinearLayout
                android:layout_width="185dp"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:weightSum="1">

                <ImageButton
                    android:id="@+id/btn_gotoweb_menu"
                    android:layout_marginTop="15dp"
                    android:layout_width="42dp"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_menu_close_clear_cancel"
                    android:background="@color/colorPrimary" />

                <ImageButton
                    android:id="@+id/btn_ticket_menu"
                    android:layout_width="39dp"
                    android:layout_height="38dp"
                    app:srcCompat="@drawable/ic_email_outline"
                    android:textColor="@android:color/white"
                    android:background="@drawable/bk_boton_plano"
                    android:stateListAnimator="@null"
                    android:layout_weight="0.22" />

            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="165dp"
            android:orientation="horizontal"
            android:weightSum="1">

            <LinearLayout
                android:layout_width="114dp"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_weight="0.04"></LinearLayout>

            <LinearLayout
                android:layout_width="185dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <Button
                    android:id="@+id/btn_etakedex_menu"
                    android:layout_width="114dp"
                    android:layout_height="wrap_content"
                    android:text="EtakÃ©dex"
                    android:textColor="@android:color/white"
                    android:background="@drawable/bk_boton_plano"
                    android:stateListAnimator="@null"/>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="162dp"
            android:orientation="horizontal">

            <LinearLayout
                android:layout_width="36dp"
                android:layout_height="match_parent"
                android:orientation="vertical"></LinearLayout>

            <LinearLayout
                android:layout_width="155dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <Button
                    android:id="@+id/btn_etakemons_menu"
                    android:layout_width="126dp"
                    android:layout_height="wrap_content"
                    android:text="Etakemons"
                    android:textColor="@android:color/white"
                    android:background="@drawable/bk_boton_plano"
                    android:stateListAnimator="@null"/>
            </LinearLayout>

            <LinearLayout
                android:layout_width="185dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <Button
                    android:id="@+id/btn_objetos_menu"
                    android:layout_width="143dp"
                    android:layout_height="wrap_content"
                    android:text="Objetos"
                    android:textColor="@android:color/white"
                    android:background="@drawable/bk_boton_plano"
                    android:stateListAnimator="@null"/>
            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal"
            android:weightSum="1">

            <LinearLayout
                android:layout_width="131dp"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:layout_weight="0.53">

            </LinearLayout>

            <LinearLayout
                android:layout_width="185dp"
                android:layout_height="match_parent"
                android:orientation="vertical">

                <ImageButton
                    android:id="@+id/btn_close_menu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_notification_clear_all"
                    android:background="@color/colorPrimaryDark"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</android.support.constraint.ConstraintLayout>
